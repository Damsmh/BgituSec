{
  "metadata": [
    {
      "src": [
        {
          "files": [ "BgituSec.Api.csproj" ],
          "src": "BgituSec.online",
          "exclude": [
            "**/Controllers/**",
            "**/Models/**",
            "**/Validators/**",
            "**/Program.cs"
          ]
        }
      ],
      "dest": "api",
      "properties": {
        "TargetFramework": "net9.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "api/**.yml", "api/**/*.md" ],
        "src": "."
      },
      {
        "files": [ "toc.yml", "*.md" ],
        "src": "."
      }
    ],
    "resource": [
      {
        "files": [ "images/**" ],
        "src": "."
      }
    ],
    "overwrite": [
      {
        "files": [ "overwrite/*.md" ],
        "src": "."
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "BGITUFIX API Documentation",
      "_appName": "BGITUFIX",
      "_appFooter": "2025 VDK",
      "_enableSearch": true,
      "_enableNewTab": true
    },
    "fileMetadata": {},
    "template": [ "default" ],
    "theme": [],
    "xref": []
  }
}